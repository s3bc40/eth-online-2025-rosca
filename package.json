{
  "name": "eth-online-2025-rosca",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=!envio-indexer",
    "dev": "turbo run dev --filter=!envio-indexer",
    "lint": "turbo run lint --filter=!envio-indexer",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "envio:dev": "turbo run envio-indexer#dev",
    "envio:codegen": "turbo run envio-indexer#codegen",
    "forge:compile": "turbo run foundry#compile",
    "anvil:setup": "turbo run anvil-states#anvil:with-safes"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "ethers": "^6.15.0",
    "lucide-react": "^0.546.0"
  }
}
